<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".FragmentParent.fragment_settings"
    android:background="@color/background_color">

    <FrameLayout
        android:id="@+id/fragmentSettingsContainer"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonLimitSetting"
        android:background="@drawable/custom_button_limit_setting"
        android:layout_width="130dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        android:text="Limit Setting"
        android:gravity="center"
        android:textSize="@dimen/text_size_18sp"
        android:textColor="@color/blue_30"
        android:fontFamily="@font/open_sans_regular"
        android:textAllCaps="false"
        />




</androidx.constraintlayout.widget.ConstraintLayout>