<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_color"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".FragmentParent.fragment_settings">

    <FrameLayout
        android:id="@+id/fragmentSettingsContainer"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <Button
        android:id="@+id/buttonLimitSetting"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="Limit Setting"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        android:textSize="@dimen/text_size_18sp"
        android:backgroundTint="@color/white_60"
        android:textColor="@color/blue_30"
        android:fontFamily="@font/open_sans_regular"
        android:textAllCaps="false"
        android:elevation="10dp"
        />




</androidx.constraintlayout.widget.ConstraintLayout>